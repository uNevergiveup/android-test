<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.antonioestrada.sesion1_myapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sesion1MyApp">
        <activity
            android:name=".EmpleadosActivity"
            android:label="Empleados"
            android:exported="false" />
        <activity
            android:name=".TiendaActivity"
            android:exported="false" />
        <activity
            android:name=".EscritorioActivity"
            android:exported="false" />
        <activity
            android:name=".OficinasActivity"
            android:exported="false"
            android:label="@string/oficinas" />
        <activity
            android:name=".AcercaDeActivity"
            android:exported="false"
            android:label="@string/acerca_de" />
        <activity
            android:name=".InformacionActivity"
            android:exported="false"
            android:label="@string/informacion" />
        <activity
            android:name=".AyudaActivity"
            android:exported="false"
            android:label="@string/ayuda" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.Sesion1MyApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/iniciar" />
        <activity
            android:name=".TerminosActivity"
            android:exported="false"
            android:label="@string/terminos" />
        <activity
            android:name=".EmpezarActivity"
            android:exported="false"
            android:theme="@style/Theme.Sesion1MyApp.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Sesion1MyApp.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>